{
  "name": "react",
  "version": "1.0.0",
  "description": "React example starter project",
  "keywords": [
    "react",
    "starter"
  ],
  "main": "src/index.js",
  "dependencies": {
    "@material-ui/core": "4.12.1",
    "axios": "0.21.1",
    "concurrently": "6.2.0",
    "json-server": "0.16.3",
    "nodemon": "2.0.12",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.4",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.0",
    "red": "0.0.0",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@babel/runtime": "7.13.8",
    "typescript": "4.1.3"
  },
    "start": "concurrently \"npm run server\" \"npm run dev\"",
    "build": "react-scripts build",
    "server": "json-server --watch db.json --port 5000",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "dev": "npm run start"
  }

  
